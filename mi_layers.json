{
    "detroit_parcels": {
        "name": "Detroit Zoning",
        "source_id": "detroit_parcels",
        "url": "mapbox://wodacooper.detroit_parcels",
        "source_layer": "hello_world",
        "layout": {
            "visibility": "none"
        },
        "visible": false,
        "line": {
            "line-width": 1,
            "line-color": "#ffffff"
        },
        "fill": {
            "fill-opacity":0.6,
            "fill-color":[
                "match",
                ["get","zoning_init"],
                "M","#7b634b",
                "R","#f2d17e",
                "S","#4A148C",
                "P","#689F38",
                "B","#f44336",
                "N","#827717",
                "#FF9800"
            ]
        },
        "legend": [
            ["M","#7b634b"],
            ["R","#f2d17e"],
            ["S","#4A148C"],
            ["P","#689F38"],
            ["B","#f44336"],
            ["N","#827717"],
            ["Other","#FF9800"]
        ],
        "hover":{
            "zoning":true,
            "owner1":true
        },
        "click": [
            "zoning",
            "parcelnum",
            "address",
            "total_acre",
            "owner1",
            "owner2",
            "own_street",
            "own_city"
        ],
        "cat_filters":{"zoning_init":"", "zoning":"","det_owned":"City Owned"},
        "cat_filter_vals":{
            "zoning_init":["R3", "R2", "R1", "R5", "M3", "SD2", "M4", "B4", "M2", "PD", "SD4", "N/A", "B2", "B3", "SD1", "B1", "R4", "B5", "PR", "R6", "TM", "M5", "B6", "PCA", "P1", "SD5", "M1", "PC", "W1"],
            "zoning":["R", "M", "S", "B", "P", "N", "T", "W"],
            "det_owned":["City Owned", "Not City Owned"]}
    },
    "detroit_preservation": {
